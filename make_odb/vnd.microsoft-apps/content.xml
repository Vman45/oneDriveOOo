:css3t="http://www.w3.org/TR/css3-text/" office:version="1.2"><office:scripts/><office:font-face-decls><style:font-face style:name="F" svg:font-family=""/></office:font-face-decls><office:automatic-styles><style:style style:name="co1" style:family="table-column" style:data-style-name="N10100"/><style:style style:name="co2" style:family="table-column"/><style:style style:name="ce1" style:family="table-cell"><style:paragraph-properties fo:text-align="start"/></style:style><number:text-style style:name="N10100" number:language="fr" number:country="FR"><number:text-content/></number:text-style></office:automatic-styles><office:body><office:database><db:data-source><db:connection-data><db:connection-resource xlink:href="sdbc:embedded:hsqldb" xlink:type="simple"/><db:login db:is-password-required="false"/></db:connection-data><db:driver-settings db:system-driver-settings="" db:base-dn="" db:parameter-name-substitution="false"/><db:application-connection-settings db:is-table-name-length-limited="false" db:append-table-alias-name="false" db:max-row-count="100"><db:table-filter><db:table-include-filter><db:table-filter-pattern>%</db:table-filter-pattern></db:table-include-filter></db:table-filter><db:data-source-settings><db:data-source-setting db:data-source-setting-is-list="false" db:data-source-setting-name="Type" db:data-source-setting-type="string"><db:data-source-setting-value>simple</db:data-source-setting-value></db:data-source-setting></db:data-source-settings></db:application-connection-settings></db:data-source><db:queries><db:query db:name="countChildTitle" db:command="SELECT COUNT( &quot;Title&quot; ) FROM &quot;Child&quot; WHERE &quot;UserId&quot; = ? AND &quot;ParentId&quot; = ? AND &quot;Title&quot; = ?"/><db:query db:name="countNewIdentifier" db:command="SELECT COUNT( &quot;Id&quot; ) &quot;Id&quot; FROM &quot;Identifiers&quot; WHERE &quot;UserId&quot; = ?"/><db:query db:name="getChildId" db:command="SELECT &quot;ItemId&quot; FROM &quot;Child&quot; WHERE &quot;UserId&quot; = ? AND &quot;ParentId&quot; = ? AND &quot;Title&quot; = ?"/><db:query db:name="getChildren" db:command="SELECT &quot;Title&quot;, &quot;Size&quot;, &quot;DateModified&quot;, &quot;DateCreated&quot;, &quot;IsFolder&quot;, CASE WHEN &quot;IsFolder&quot; = TRUE THEN CONCAT( ?, CONCAT( &apos;/&apos;, &quot;ItemId&quot; ) ) ELSE CONCAT( ?, CONCAT( &apos;/&apos;, &quot;Title&quot; ) ) END &quot;TargetURL&quot;, FALSE &quot;IsHidden&quot;, FALSE &quot;IsVolume&quot;, FALSE &quot;IsRemote&quot;, FALSE &quot;IsRemoveable&quot;, FALSE &quot;IsFloppy&quot;, FALSE &quot;IsCompactDisc&quot; FROM &quot;Child&quot; WHERE &quot;UserId&quot; = ? AND &quot;ParentId&quot; = ? AND ( &quot;IsFolder&quot; = TRUE OR &quot;Loaded&quot; &gt;= ? )"/><db:query db:name="getItem" db:command="SELECT &quot;ItemId&quot; &quot;Id&quot;, &quot;Title&quot;, &quot;Title&quot; &quot;TitleOnServer&quot;, &quot;DateCreated&quot;, &quot;DateModified&quot;, &quot;ContentType&quot;, &quot;MediaType&quot;, &quot;Size&quot;, &quot;Trashed&quot;, &quot;IsRoot&quot;, &quot;IsFolder&quot;, &quot;IsDocument&quot;, TRUE &quot;CanAddChild&quot;, TRUE &quot;CanRename&quot;, FALSE &quot;IsReadOnly&quot;, FALSE &quot;IsVersionable&quot;, &quot;Loaded&quot;, &apos;&apos; &quot;CasePreservingURL&quot;, FALSE &quot;IsHidden&quot;, FALSE &quot;IsVolume&quot;, FALSE &quot;IsRemote&quot;, FALSE &quot;IsRemoveable&quot;, FALSE &quot;IsFloppy&quot;, FALSE &quot;IsCompactDisc&quot; FROM &quot;Item&quot; WHERE &quot;UserId&quot; = ? AND &quot;ItemId&quot; = ?"/><db:query db:name="getItemToSync" db:command="SELECT &quot;C&quot;.&quot;ItemId&quot; &quot;id&quot;, &quot;C&quot;.&quot;Title&quot; &quot;name&quot;, &quot;C&quot;.&quot;DateCreated&quot; &quot;createdDateTime&quot;, &quot;C&quot;.&quot;DateModified&quot; &quot;DateModified&quot;, &quot;C&quot;.&quot;MediaType&quot;, &quot;C&quot;.&quot;Size&quot; &quot;size&quot;, &quot;C&quot;.&quot;ParentId&quot; &quot;parent&quot;, &quot;C&quot;.&quot;IsRoot&quot;, &quot;C&quot;.&quot;AtRoot&quot;, &quot;S&quot;.&quot;SyncMode&quot; &quot;Mode&quot;, &quot;S&quot;.&quot;SyncId&quot; FROM &quot;Synchronizes&quot; &quot;S&quot; LEFT JOIN &quot;Child&quot; &quot;C&quot; ON &quot;S&quot;.&quot;UserId&quot; = &quot;C&quot;.&quot;UserId&quot; AND &quot;S&quot;.&quot;ItemId&quot; = &quot;C&quot;.&quot;ItemId&quot; WHERE &quot;S&quot;.&quot;UserId&quot; = ? ORDER BY &quot;S&quot;.&quot;SyncId&quot;"/><db:query db:name="getNewIdentifier" db:command="SELECT &quot;Id&quot; FROM &quot;Identifiers&quot; WHERE &quot;UserId&quot; = ? ORDER BY &quot;TimeStamp&quot; LIMIT 1"/><db:query db:name="getSetting" db:command="SELECT &quot;Set2&quot;, &quot;Set3&quot; FROM &quot;Settings&quot; WHERE &quot;Setting&quot; = ?"/><db:query db:name="getUser" db:command="SELECT &quot;U&quot;.&quot;UserId&quot;, &quot;U&quot;.&quot;RootId&quot;, &quot;I&quot;.&quot;Title&quot; &quot;RootName&quot; FROM &quot;Users&quot; &quot;U&quot; JOIN &quot;Items&quot; &quot;I&quot; ON &quot;U&quot;.&quot;RootId&quot; = &quot;I&quot;.&quot;ItemId&quot; WHERE &quot;U&quot;.&quot;UserName&quot; = ?"/><db:query db:name="isChildId" db:command="SELECT CAST( COUNT( 1 ) AS &quot;BOOLEAN&quot; ) &quot;IsChild&quot; FROM &quot;Parents&quot; WHERE &quot;UserId&quot; = ? AND &quot;ChildId&quot; = ? AND &quot;ItemId&quot; = ?"/><db:query db:name="isIdentifier" db:command="SELECT CAST( COUNT( 1 ) AS &quot;BOOLEAN&quot; ) &quot;IsIdentifier&quot; FROM &quot;Items&quot; WHERE &quot;ItemId&quot; = ?"/></db:queries><db:table-representations><db:table-representation db:name="Settings"/><db:table-representation db:name="Users"/><db:table-representation db:name="Items"/><db:table-representation db:name="Capabilities"/><db:table-representation db:name="Parents"/></db:table-representations></office:database></office:body></office:document-content>